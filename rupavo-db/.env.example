# Supabase Connection (Required for seeders and local development)
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
SUPABASE_DB_URL="postgresql://postgres.[YOUR-PROJECT-REF]:[YOUR-PASSWORD]@aws-0-[REGION].pooler.supabase.com:6543/postgres"

# Admin User (for seeders)
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=securepassword

# ============================================
# Edge Function Secrets
# ============================================
# Set these using: npx supabase secrets set KEY=value
# Or via Supabase Dashboard: Settings > Edge Functions > Secrets

# Midtrans Payment Gateway (Required for payment features)
# Used by: payment-create, payment-webhook
# Get credentials from: https://dashboard.midtrans.com/
# SANDBOX credentials (for testing):
MIDTRANS_SERVER_KEY=SB-Mid-server-xxxxxxxxxxxxx
MIDTRANS_CLIENT_KEY=SB-Mid-client-xxxxxxxxxxxxx
MIDTRANS_IS_PRODUCTION=false
# PRODUCTION credentials (for live transactions):
# MIDTRANS_SERVER_KEY=Mid-server-xxxxxxxxxxxxx
# MIDTRANS_CLIENT_KEY=Mid-client-xxxxxxxxxxxxx
# MIDTRANS_IS_PRODUCTION=true

# Kolosal AI API (Optional - for kolosal-proxy function)
# Get your API key from: https://kolosal.ai/
KOLOSAL_API_KEY=kol__your_kolosal_api_key_here

# Other Optional Services
SECRET_VALUE=your-secret-value
OPENAI_API_KEY=your-openai-api-key
SENDGRID_API_KEY=your-sendgrid-api-key
SUPABASE_AUTH_SMS_TWILIO_AUTH_TOKEN=your-twilio-auth-token
SUPABASE_AUTH_EXTERNAL_APPLE_SECRET=your-apple-secret
S3_HOST=your-s3-host
S3_REGION=your-s3-region
S3_ACCESS_KEY=your-s3-access-key
S3_SECRET_KEY=your-s3-secret-key

ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=securepassword

# Kolosal AI API Key (starts with 'kol__')
KOLOSAL_API_KEY=kol__your_kolosal_api_key_here
 
SUPABASE_DB_URL="postgresql://postgres.[YOUR-PROJECT-REF]:[YOUR-PASSWORD]@aws-0-[REGION].pooler.supabase.com:6543/postgres" 
